            <li>loadkmap</li>
            <li>makedevs</li>
            <li>mdev</li>
            <li>popmaildir</li>
            <li>raidautorun</li>
            <li>rdev</li>
            <li>readahead</li>
            <li>run-init</li>
            <li>setconsole</li>
            <li>tftpd</li>
            <li>uevent</li>
            <li>zcip</li>
        </ul>
    </description>

    <requires>
        <package name="busybox"/>
    </requires>

