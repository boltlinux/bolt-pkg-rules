<?xml version="1.0" encoding="utf-8"?>
<package name="libc6-dev" section="libdevel">
    <description>
        <summary>GNU C Library: Development Libraries and Header Files</summary>
        <p>
        Contains the symlinks, headers, and object files needed to compile
        and link programs which use the standard C library.
        </p>
    </description>

    <requires>
        <package name="libc-dev-bin" version="=="/>
        <package name="libcrypt-dev"/>
        <package name="libnsl-dev"/>
    </requires>

    <contents>
        <file src="/usr/include"/>

        <file src="/usr/lib/Mcrt1.o"/>
        <file src="/usr/lib/Scrt1.o"/>
        <file src="/usr/lib/crt1.o"/>
        <file src="/usr/lib/crti.o"/>
        <file src="/usr/lib/crtn.o"/>
        <file src="/usr/lib/gcrt1.o"/>
        <file src="/usr/lib/grcrt1.o"/>
        <file src="/usr/lib/rcrt1.o"/>

        <file src="/usr/lib/*.a"/>

        <file src="/usr/lib/libBrokenLocale.a"/>
        <file src="/usr/lib/libBrokenLocale.so"/>

        <file src="/usr/lib/libanl.so"/>
        <file src="/usr/lib/libc.so"/>
        <file src="/usr/lib/libdl.so"/>
        <file src="/usr/lib/libm.so"/>
        <file src="/usr/lib/libmvec.so"/>
        <file src="/usr/lib/libnss_compat.so"/>
        <file src="/usr/lib/libnss_dns.so"/>
        <file src="/usr/lib/libnss_files.so"/>
        <file src="/usr/lib/libnss_hesiod.so"/>
        <file src="/usr/lib/libpthread.so"/>
        <file src="/usr/lib/libresolv.so"/>
        <file src="/usr/lib/librt.so"/>
        <file src="/usr/lib/libthread_db.so"/>
        <file src="/usr/lib/libutil.so"/>

        <file src="/usr/share/gdb/auto-load/lib/*-linux-gnu"/>
    </contents>
</package>
