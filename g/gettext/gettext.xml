        <file src="/usr/bin/msgcmp"/>
        <file src="/usr/bin/msgcomm"/>
        <file src="/usr/bin/msgconv"/>
        <file src="/usr/bin/msgen"/>
        <file src="/usr/bin/msgexec"/>
        <file src="/usr/bin/msgfilter"/>
        <file src="/usr/bin/msgfmt"/>
        <file src="/usr/bin/msggrep"/>
        <file src="/usr/bin/msginit"/>
        <file src="/usr/bin/msgmerge"/>
        <file src="/usr/bin/msgunfmt"/>
        <file src="/usr/bin/msguniq"/>
        <file src="/usr/bin/recode-sr-latin"/>
        <file src="/usr/bin/xgettext"/>
        <file src="/usr/lib/gettext"/>
        <file src="/usr/lib/libgettextlib-0.19.8.1.so"/>
        <file src="/usr/lib/libgettextsrc-0.19.8.1.so"/>
        <file src="/usr/lib/preloadable_libintl.so"/>
        <file src="/usr/share/aclocal"/>
