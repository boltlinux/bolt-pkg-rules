<?xml version="1.0" encoding="utf-8"?>
<package name="gettext" section="devel">
    <description>
        <summary>GNU Internationalization utilities</summary>
        <p>
        Interesting for authors or maintainers of other packages or programs
        which they want to see internationalized.
        </p>
    </description>

    <requires>
        <package name="gettext-base"/>
    </requires>

    <contents>
        <file src="${prefix}/bin/gettextize"/>
        <file src="${prefix}/bin/msgattrib"/>
        <file src="${prefix}/bin/msgcat"/>
        <file src="${prefix}/bin/msgcmp"/>
        <file src="${prefix}/bin/msgcomm"/>
        <file src="${prefix}/bin/msgconv"/>
        <file src="${prefix}/bin/msgen"/>
        <file src="${prefix}/bin/msgexec"/>
        <file src="${prefix}/bin/msgfilter"/>
        <file src="${prefix}/bin/msgfmt"/>
        <file src="${prefix}/bin/msggrep"/>
        <file src="${prefix}/bin/msginit"/>
        <file src="${prefix}/bin/msgmerge"/>
        <file src="${prefix}/bin/msgunfmt"/>
        <file src="${prefix}/bin/msguniq"/>
        <file src="${prefix}/bin/recode-sr-latin"/>
        <file src="${prefix}/bin/xgettext"/>
        <file src="${prefix}/lib/gettext"/>
        <file src="${prefix}/lib/libgettextlib-*.*.so"/>
        <file src="${prefix}/lib/libgettextsrc-*.*.so"/>
        <file src="${prefix}/share/aclocal"/>
        <file src="${prefix}/share/gettext-*.*/its/**/*"/>
        <file src="${prefix}/share/gettext/ABOUT-NLS"/>
        <file src="${prefix}/share/gettext/config.rpath" mode="0755"/>
        <file src="${prefix}/share/gettext/gettext.h"/>
        <file src="${prefix}/share/gettext/intl"/>
        <file src="${prefix}/share/gettext/javaversion.class"/>
        <file src="${prefix}/share/gettext/msgunfmt.tcl"/>
        <file src="${prefix}/share/gettext/po"/>
        <file src="${prefix}/share/gettext/projects"/>
        <file src="${prefix}/share/gettext/styles"/>
    </contents>
</package>
