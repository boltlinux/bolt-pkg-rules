<?xml version="1.0" encoding="utf-8"?>
<package name="gettext" section="devel">
    <description>
        <summary>GNU Internationalization utilities</summary>
        <p>
        Interesting for authors or maintainers of other packages or programs
        which they want to see internationalized.
        </p>
    </description>

    <requires>
        <package name="gettext-base"/>
    </requires>

    <contents>
        <file src="/usr/bin/gettextize"/>
        <file src="/usr/bin/msgattrib"/>
