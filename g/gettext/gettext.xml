        <file src="/usr/share/gettext/javaversion.class"/>
        <file src="/usr/share/gettext/msgunfmt.tcl"/>
        <file src="/usr/share/gettext/po"/>
        <file src="/usr/share/gettext/projects"/>
        <file src="/usr/share/gettext/styles"/>
        <dir src="/usr/share/java"/>
        <file src="/usr/share/java/gettext.jar"/>
