<?xml version="1.0" encoding="utf-8"?>
<package name="libgnutls28-dev" section="libdevel">
    <description>
        <summary>GNU TLS library - development files</summary>
        <p>
        GnuTLS is a portable library which implements the Transport Layer
        Security (TLS 1.0, 1.1, 1.2) and Secure Sockets Layer (SSL) 3.0 and Datagram
        Transport Layer Security (DTLS 1.0, 1.2) protocols.
        </p>
        <p>
        This package contains the GnuTLS development files.
        </p>
    </description>

    <requires>
        <package name="libgnutls30" version="=="/>
        <package name="libgnutls-openssl27" version="=="/>
        <package name="libgnutlsxx28" version="=="/>
        <package name="libgnutls-dane0" version="=="/>
        <package name="nettle-dev"/>
        <package name="musl-libc-dev"/>
        <package name="zlib1g-dev"/>
        <package name="libtasn1-6-dev"/>
        <package name="libp11-kit-dev"/>
        <package name="libidn11-dev" version=">= 1.31"/>
    </requires>

    <contents>
        <file src="${prefix}/include/gnutls"/>
        <file src="${prefix}/lib/libgnutls-dane.a"/>
        <file src="${prefix}/lib/libgnutls-dane.so"/>
        <file src="${prefix}/lib/libgnutls-openssl.a"/>
        <file src="${prefix}/lib/libgnutls-openssl.so"/>
        <file src="${prefix}/lib/libgnutls.a"/>
        <file src="${prefix}/lib/libgnutls.so"/>
        <file src="${prefix}/lib/libgnutlsxx.a"/>
        <file src="${prefix}/lib/libgnutlsxx.so"/>
        <file src="${prefix}/lib/pkgconfig"/>
    </contents>
</package>
