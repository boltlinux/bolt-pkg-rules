<?xml version="1.0" encoding="utf-8"?>
<package name="libdb5.3-sql-dev" section="libdevel">
    <description>
        <summary>Berkeley v5.3 Database Libraries [SQL development]</summary>
        <p>
        This is the development package which contains headers and static
        libraries for the Berkeley v5.3 database library.
        </p>
    </description>

    <requires>
        <package name="libdb5.3-sql" version="=="/>
    </requires>

    <contents>
        <file src="${prefix}/include/dbsql.h"/>
        <file src="${prefix}/lib/libdb_sql-5.3.a"/>
        <file src="${prefix}/lib/libdb_sql.a"/>
        <file src="${prefix}/lib/libdb_sql.so"/>
    </contents>
</package>
