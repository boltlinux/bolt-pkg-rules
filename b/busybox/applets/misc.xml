<?xml version="1.0" encoding="utf-8"?>
<package name="busybox-misc-symlinks" section="admin">
    <description>
        <summary>Uncategorized busybox symlinks</summary>
        <p>
            This package contains the symbolic links for the following applets:
        </p>
        <ul>
            <li>catv</li>
            <li>cryptpw</li>
            <li>cttyhack</li>
            <li>dumpkmap</li>
            <li>fsync</li>
            <li>ftpget</li>
            <li>ftpput</li>
            <li>nmeter</li>
            <li>pipe_progress</li>
            <li>ttysize</li>
            <li>udhcpc6</li>
            <li>usleep</li>
            <li>crond</li>
            <li>devmem</li>
            <li>dhcprelay</li>
            <li>dnsd</li>
            <li>ether-wake</li>
            <li>fakeidentd</li>
            <li>fbsplash</li>
            <li>httpd</li>
            <li>inotifyd</li>
            <li>ipaddr</li>
            <li>iplink</li>
            <li>iproute</li>
            <li>iprule</li>
            <li>loadfont</li>
            <li>loadkmap</li>
            <li>makedevs</li>
            <li>raidautorun</li>
            <li>rdev</li>
            <li>readahead</li>
            <li>setconsole</li>
            <li>tftpd</li>
            <li>zcip</li>
        </ul>
    </description>

    <requires>
        <package name="base-files"/>
        <package name="busybox"/>
    </requires>

    <contents>
        <file src="${prefix}/bin/["/>
        <file src="${prefix}/bin/[["/>
        <file src="${prefix}/bin/catv"/>
        <file src="${prefix}/bin/cryptpw"/>
        <file src="${prefix}/bin/cttyhack"/>
        <file src="${prefix}/bin/dumpkmap"/>
        <file src="${prefix}/bin/fsync"/>
        <file src="${prefix}/bin/ftpget"/>
        <file src="${prefix}/bin/ftpput"/>
        <file src="${prefix}/bin/nmeter"/>
        <file src="${prefix}/bin/pipe_progress"/>
        <file src="${prefix}/bin/ttysize"/>
        <file src="${prefix}/bin/udhcpc6"/>
        <file src="${prefix}/bin/usleep"/>
        <file src="${prefix}/sbin/crond"/>
        <file src="${prefix}/sbin/devmem"/>
        <file src="${prefix}/sbin/dhcprelay"/>
        <file src="${prefix}/sbin/dnsd"/>
        <file src="${prefix}/sbin/ether-wake"/>
        <file src="${prefix}/sbin/fakeidentd"/>
        <file src="${prefix}/sbin/fbsplash"/>
        <file src="${prefix}/sbin/httpd"/>
        <file src="${prefix}/sbin/inotifyd"/>
        <file src="${prefix}/sbin/ipaddr"/>
        <file src="${prefix}/sbin/iplink"/>
        <file src="${prefix}/sbin/iproute"/>
        <file src="${prefix}/sbin/iprule"/>
        <file src="${prefix}/sbin/loadfont"/>
        <file src="${prefix}/sbin/loadkmap"/>
        <file src="${prefix}/sbin/makedevs"/>
        <file src="${prefix}/sbin/raidautorun"/>
        <file src="${prefix}/sbin/rdev"/>
        <file src="${prefix}/sbin/readahead"/>
        <file src="${prefix}/sbin/setconsole"/>
        <file src="${prefix}/sbin/tftpd"/>
        <file src="${prefix}/sbin/zcip"/>
    </contents>
</package>
